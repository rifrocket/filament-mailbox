{
    "name": "rifrocket/filament-mailbox",
    "description": "A synchronous, advanced mailbox plugin for Laravel Filament 3 offering a robust webmail client experience similar to Gmail/Hotmail.",
    "type": "filament-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Mohammad Arif ( RifRocket )",
            "email": "mohammad.arif9999@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^11.0",
        "filament/filament": "^3.2",
        "webklex/laravel-imap": "^4.1.2"
    },
    "autoload": {
        "psr-4": {
            "RifRocket\\FilamentMailbox\\": "src/",
            "Database\\Factories\\": "database/factories/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "RifRocket\\FilamentMailbox\\Providers\\MailboxPluginServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

